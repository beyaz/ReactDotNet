<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="PrimeReact\AutoComplete.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="ReactDotNet.js" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="ReactDotNet.js">
